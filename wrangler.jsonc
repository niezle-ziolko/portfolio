{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "portfolio",
	"main": ".open-next/worker.js",
	"workers_dev": true,
	"compatibility_date": "2025-03-01",
	"compatibility_flags": [
		"nodejs_compat",
		"global_fetch_strictly_public"
	],
	"dev": {
		"ip": "127.0.0.1",
    	"port": 3000,
    	"local_protocol": "http"
	},
  	"placement": {
		"mode": "smart"
  	},
	"assets": {
		"binding": "ASSETS",
		"directory": ".open-next/assets"
	},
	"observability": {
		"enabled": true
	},
    "vars": {
	    "NODE_VERSION": "25.1.0",
        "PNPM_VERSION": "10.23.0"
	}
}